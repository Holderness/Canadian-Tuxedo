<%= form_for(@user) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.submit :submit %>
<% end %>


<%= form_for @user, :url => users_path, :html => { :multipart => true } do |form| %>
  <%= form.file_field :clothing_item %>
<% end %>